style"],"sourceRoot":"src/components","sources":["mymessages.vue"],"sourcesContent":["<template>\r\n\r\n\r\n    <div>\r\n\r\n        <h1 style=\"text-align: center;font-size: 25px;color:deepskyblue\">留言墙(懒得回复0.0)</h1>\r\n        <div class=\"infinite-list-wrapper\" style=\"overflow:auto\">\r\n\r\n            <dl\r\n                    class=\"list\"\r\n                    v-infinite-scroll=\"load\"\r\n                    infinite-scroll-disabled=\"disabled\"\r\n                    style=\"text-align: center\">\r\n                <dt v-for=\"i in message\" class=\"list-item \" style=\"line-height: 30px\">\r\n                    <div style=\"width: 100%;height: 100px\" class=\"ok\">\r\n                    <p style=\"text-align: left;color:skyblue\">{{i.user}}:({{i.create_time}})</p>\r\n                    <p style=\"text-align: left;color:mediumturquoise\">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;{{i.content}}</p>\r\n                </div></dt>\r\n            </dl>\r\n            <p v-if=\"loading\">加载中...</p>\r\n            <p v-if=\"noMore\">没有更多了</p>\r\n\r\n            <el-input\r\n  type=\"text\"\r\n  placeholder=\"昵称\"\r\n  v-model=\"name\"\r\n  maxlength=\"10\"\r\n  show-word-limit\r\n>\r\n</el-input>\r\n<div style=\"margin: 20px 0;\"></div>\r\n            <div class=\"el-col-20\"><el-input\r\n  type=\"textarea\"\r\n  placeholder=\"在这里写留言吧!\"\r\n  v-model=\"textarea\"\r\n  maxlength=\"30\"\r\n  show-word-limit\r\n>\r\n</el-input></div>\r\n            <div class=\"el-col-4\">\r\n                <el-button type=\"primary\" style=\"float:right\" @click=\"send_message\">提交</el-button>\r\n            </div>\r\n\r\n        </div>\r\n\r\n    </div>\r\n\r\n</template>\r\n\r\n<script>\r\n    import settings from \"../assets/js/settings\";\r\n    export default {\r\n        name: \"mymessages\",\r\n        data() {\r\n            return {\r\n                count: 10,\r\n                loading: false,\r\n                message:[],\r\n                name: '',\r\n                textarea: ''\r\n            }\r\n        },\r\n        created() {\r\n          this.get_message();\r\n        },\r\n        computed: {\r\n            noMore() {\r\n                return this.count >= 10\r\n            },\r\n            disabled() {\r\n                return this.loading || this.noMore\r\n            }\r\n        },\r\n        methods: {\r\n            load() {\r\n                this.loading = true\r\n                setTimeout(() => {\r\n                    this.count += 2\r\n                    this.loading = false\r\n                }, 2000)\r\n            },\r\n            get_message(){\r\n                this.$axios.get(settings.base_url+'/user/message/').then(response=>{\r\n                    this.message = response.data;\r\n                }).catch(error=>{\r\n\r\n                })\r\n            },\r\n            send_message(){\r\n                if(this.name == ''||this.message==''){\r\n                    this.$message({\r\n                        message:'缺一不可哦,小笨蛋!',\r\n                        type:'warning'\r\n                    })\r\n                }\r\n                else{\r\n                    this.$axios.post(settings.base_url+'/user/message/',{\r\n                    \"user\":this.name,\r\n                    \"content\":this.textarea\r\n                }).then(response=>{\r\n                    console.log('success');\r\n\r\n                    this.get_message()\r\n                    this.open();\r\n\r\n                }).catch(error=>{})\r\n                console.log('fail')\r\n                }\r\n\r\n            },\r\n                  open() {\r\n        const h = this.$createElement;\r\n\r\n        this.$notify({\r\n          title: '小梦琦告诉你:',\r\n          message: h('i', { style: 'color: teal'}, '留言成功')\r\n        });\r\n      },\r\n        }\r\n    };\r\n\r\n\r\n</script>\r\n\r\n<style scoped>\r\n.ok{\r\n    box-shadow: 0 2px 12px 0 rgba(0, 0, 0, 0.1)\r\n}\r\n</style>"]}]}